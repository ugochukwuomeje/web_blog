<table>
<tr><td>title</td><td>description</td><td>show</td><td>edit</td><td>delete</td></tr>
<% @articles.each do |item| %>
  <tr><td><%= item.title%> </td><td><%= item.description%></td><td><%= link_to 'show', article_path(item) %></td><td><%= link_to 'edit', edit_article_path(item) %></td>
  <td><%= link_to 'Delete', article_path(item), method: :delete, data: {confirm: "Are you sure?"}  %></td></tr>
<% end %>
</table>